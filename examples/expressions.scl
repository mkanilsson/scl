use std::io::printf

interface Print {
    proc print(this) void  
}

struct Thing {
    a: i32
}

impl Print for Thing {
    proc print(this) void {
        printf("Value: %d\n", this.a);
        ret;
    } 
}

proc main() void {
    let t = Thing {
        a: 69
    };
    
    generic(t);

    ret;
}

proc generic<T: Print>(value: T) void {
    value.print();
    ret;
}
